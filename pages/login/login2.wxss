/* pages/login/login2.wxss */
.containerLogin{
  background: #ebebeb;
  font-size: 36rpx;
  width: 100%;
  height: 100%;
  position: relative;
}
.bottom-area{
  width: 100%;
  height: 300rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
  box-sizing: border-box;
  padding: 40rpx;
}
.logo{
  width: 250rpx;
  height: 50rpx;
}
.point{
  width: 44rpx;
  height: 44rpx;
  margin-right: 10rpx;
}
.content{
  padding: 0 24rpx;
}
.content>view:nth-last-child(1){
  margin-bottom: 100rpx;
}
.btn{
  width: 100%;
  height: 100rpx;
  background-color: #0f4c83;
  font-size: 34rpx;
  line-height: 100rpx;
  border-radius: 50rpx;
  font-weight: 600;
  margin-top: 30rpx;
}
.interestTitle{
  color: #666;
  margin-left: 24rpx;
  margin-top: 50rpx;
}
.interestList>view{
  float: left;
  border: 1rpx solid #0f4c83;
  color: #0f4c83;
  padding: 12rpx 30rpx;
  border-radius: 10rpx;
  margin: 10rpx 16rpx;
  font-size: 24rpx;
}
.interestList>.interestListActive{
  background: #0f4c83;
  color: #fff;
}

.btn-clean{
  padding: inherit;
  margin: inherit;
}
